{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch api",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}\\src\\api",
            "env": {
                "PORT": "80"
            }
        },
        {
            "name": "fcpxml_web",
            "cwd": "src\\fcpxml_web",
            "request": "launch",
            "type": "dart",
            "args": [
                "--web-browser-flag=--disable-web-security"
            ]
        },
        {
            "name": "fcpxml_web (profile mode)",
            "cwd": "src\\fcpxml_web",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
        },
        {
            "name": "fcpxml_web (release mode)",
            "cwd": "src\\fcpxml_web",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release"
        }
    ]
}